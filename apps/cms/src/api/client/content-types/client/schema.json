{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "socialUrl": {
      "type": "string",
      "required": true
    },
    "video_entries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::video-entry.video-entry",
      "mappedBy": "client"
    }
  }
}
